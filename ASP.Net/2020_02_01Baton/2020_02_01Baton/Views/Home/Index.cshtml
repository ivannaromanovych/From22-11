@model IList<_2020_02_01Baton.Models.ProductModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1 class="mt-3 text-center">
    Bakery
</h1>
<div class="row">
    @foreach(var i in Model)
    {
         <div class="col-md-4 col-lg-3">
             <div class="card w-auto p-3">
                 @*<a class="" href="@i.Image" data-lightbox="example-set" data-title="@i.Id">*@
                      <img class="card-img-top" src="@i.Image" alt="@i.Name">
                 @*</a>*@
                 <div class="card-body">
                     <p class="card-text">
                         @i.Name
                     </p>
                 </div>
              </div>
         </div>
    }
</div>
